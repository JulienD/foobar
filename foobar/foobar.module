<?php

/**
 * Implements hook_menu().
 */
function foobar_menu() {
	// In general the path doesn't really matter since it is already registered in foobar.routing.yml
	$items['admin/foobar'] = array(
		'title' => 'Foobar',
		'description' => 'This is the demo foobar page.',
		// This is the name of the route from foobar.routing.yml
		'route_name' => 'foobar',
	);
  return $items;
}
